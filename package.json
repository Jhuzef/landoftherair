{
  "name": "landoftherair",
  "version": "1.0.0",
  "description": "a prototype morpg",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:server:plain": "ts-node src/server/index.ts",
    "start:server:plain:debug": "ts-node --inspect src/server/index.ts",
    "start:server": "nodemon --ignore \"client/\" --ignore \"dist/*\" --ignore \"*.json\" --exec \"ts-node \" -- \"src/server/index.ts\"",
    "start:server:single": "nodemon --ignore \"client/\" --ignore \"dist/*\" --ignore \"*.json\" --exec \"ts-node \" -- \"src/server/index.ts\" --single-core",
    "start:server:debug": "nodemon --ignore \"client/\" --ignore \"dist/*\" --ignore \"*.json\" --exec \"ts-node --inspect \" -- \"src/server/index.ts\" --single-core",
    "start:client": "ng serve -o --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:netlify": "npm run postinstall && npm run build:prod",
    "prebuild:prod": "node scripts/prebuild-client",
    "build:prod": "ng build --environment=prod --vendor-chunk=true --output-hashing=all --sourcemaps --extract-css --extract-licenses",
    "deploy": "npm run test:local && npm run prod:server && npm run prod:deploy",
    "prod:server": "node scripts/compile",
    "prod:deploy": "node scripts/deploy",
    "prod:start": "node dist/server/index",
    "postinstall": "node scripts/postinstall-assets",
    "lint": "ng lint",
    "test:unit": "ava-ts src/**/*.test.ts",
    "test:lint": "npm run lint",
    "test:local": "npm run test:unit && npm run test:lint",
    "test": "npm run test:local && npm run build:prod && npm run task:seed",
    "setup": "npm-run-all task:*",
    "seed:items": "ts-node src/server/tasks/items.ts",
    "seed:npcs": "ts-node src/server/tasks/npcs.ts",
    "seed:drops": "ts-node src/server/tasks/drops.ts",
    "seed:recipes": "ts-node src/server/tasks/recipes.ts",
    "task:seed": "npm-run-all seed:*",
    "task:macros": "npm-run-all task:macros:*",
    "task:macros:data": "ts-node src/server/tasks/macroiconnames.ts",
    "task:macros:meta": "ts-node src/server/tasks/macrometadata.ts",
    "task:macros:font": "webfont -f macicons -t css -d src/client/macicons src/client/assets/icons/*.svg",
    "task:allsprites": "ts-node src/server/tasks/analysis/allsprites",
    "task:analysis": "npm-run-all analyze:*",
    "analyze:weapons": "ts-node src/server/tasks/analysis/weapons",
    "analyze:armor": "ts-node src/server/tasks/analysis/armor",
    "analyze:premium": "ts-node src/server/tasks/analysis/premium",
    "runseed:prod": "npm-run-all \"seed:* -- --prod\"",
    "bump:patch": "npm version patch -m \"Patch release %s\"",
    "bump:minor": "npm version minor -m \"Minor release %s\"",
    "bump:major": "npm version major -m \"Major release %s\"",
    "changelog": "gh-change-gen --owner landoftherair --repo landoftherair --labels enhancement,bug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seiyria/kod-prototype.git"
  },
  "engines": {
    "node": "8.6.0"
  },
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/seiyria/kod-prototype/issues"
  },
  "homepage": "https://github.com/seiyria/kod-prototype#readme",
  "dependencies": {
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@angular/forms": "^5.2.6",
    "@angular/http": "^5.2.6",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/router": "^5.2.6",
    "angular-resizable-element": "^2.0.0",
    "angular2-auto-scroll": "^1.0.12",
    "auth0-js": "^8.10.1",
    "bootstrap": "^4.0.0-beta",
    "censor-sensor": "^1.0.3",
    "cli-table": "^0.3.1",
    "colyseus": "^0.8.0",
    "colyseus.js": "^0.8.0",
    "connect-static-file": "^1.2.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "dice.js": "^0.9.3",
    "discord.js": "^11.3.0",
    "dotenv": "^4.0.0",
    "download-github-repo": "^0.1.4",
    "express": "^4.15.3",
    "fantastical": "^1.0.1",
    "jsonwebtoken": "^7.4.1",
    "jwks-rsa": "^1.2.1",
    "linkifyjs": "^2.1.4",
    "lodash": "^4.17.4",
    "lootastic": "^1.0.2",
    "luxon": "^0.2.11",
    "mingy": "^0.2.7",
    "mongodb": "^2.2.27",
    "mrpas": "^1.2.1",
    "ng-drag-drop": "^4.0.0",
    "ng-stripe-checkout": "^1.0.2",
    "ngx-bootstrap": "^2.0.2",
    "ngx-color-picker": "^5.3.3",
    "ngx-webstorage": "^2.0.1",
    "node-redis-pubsub": "^1.0.3",
    "node-schedule": "^1.2.3",
    "nonenumerable": "^1.1.0",
    "pathfinding": "^0.4.18",
    "phaser-ce": "^2.8.0",
    "phaser-tiled": "^2.0.4",
    "process-stats": "^2.0.0",
    "recursive-readdir": "^2.2.1",
    "restricted-number": "^1.1.0",
    "rollbar": "2.1.0",
    "roman-numerals": "^0.3.2",
    "rxjs": "^5.5.6",
    "signals.js": "^1.0.1-alpha.3",
    "stripe": "^5.4.0",
    "sweetalert2": "^6.10.2",
    "ts-node": "^5.0.0",
    "uuid": "^3.1.0",
    "uws": "^9.14.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.1",
    "@angular/compiler-cli": "^5.2.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "ava-ts": "^0.23.0",
    "codelyzer": "~2.0.0",
    "fs-extra": "^4.0.2",
    "md5-file": "^3.2.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "tslint": "~4.5.0",
    "typescript": "^2.4.2",
    "webfont": "^7.1.4",
    "webpack": "^3.3.0",
    "yamljs": "^0.2.10"
  }
}
